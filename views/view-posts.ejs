<link rel="stylesheet" href="stylesheets/style.css" />
<% for (let index = 0; index < posts.length; index++) {%>
<div class="post">
    <h1> <%= posts[index].title %></h1>
    <p> <%= posts[index].date %></p>
    <p>By <%= posts[index].author %></p>
    <p> <%= posts[index].post %></p>
</div>
<% } %>